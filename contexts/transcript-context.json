{
        "@context": {
          "@version": 1.1,
          "@protected": true,
          "id": "@id",
          "type": "@type",
          "schema": "https://schema.org/",
          "igovtt": "https://igovtt.github.io/schemas/",
          "SkillCertificate":{
            "@id": "igovtt:SkillCertificate",
            "@context": {
              "@version": 1.1,
              "@protected": true
            }
          },
          "Student": {
            "@id": "schema:Person",
            "@context": {
              "@version": 1.1,
              "@protected": true,
              "name": "schema:name",
              "UIN": "schema:identifier",
              "gender": "schema:gender",
              "mobile": "schema:telephone",
              "email": "schema:email",
              "registrationDate": "schema:startDate"
            }
        
          },
          "Course": {
            "@id": "schema:Course",
            "@context": {
              "@version": 1.1,
              "@protected": true,
              "name": "schema:name",
              "level": "schema:educationalLevel",
              "status": "schema:StatusEnumeration",
              "expiryDate": "schema:expires",
              "issuedDate": "schema:dateIssued"
            }
        
          },
          "student": "igovtt:Student",
          "courses": "igovtt:Courses"
        }
      }
